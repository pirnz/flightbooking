<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="FlightBookings.workshop..controller.FlightBookings" displayBlock="true">
    <Shell id="shell">
        <App id="app">
            <pages>
                <Page id="page" title="{i18n>title}">
                    <content>
                        <List noDataText="Drop list items here" id="list0" items="{/zsbookSet}">
                            <items>
                                <StandardListItem type="Navigation" title="Booking ID: {Bookid} Passenger Name: {Passname}" description="{Forcuram}" icon="sap-icon://picture" id="item0"/>
                            </items>
                        </List>
                    </content>
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>